+void  RingBuffer::putBuf(Buffer *buf, int wake)
+{
+    mBufLock.lock();
+    if(isFull()){
+        // to do error
+    }
+    rd_index += buf->mSize;
+    if(wake){
+        mBufCond.signal();
+    }
+    mBufLock.unlock();
+    mLock_rd.unlock();
+    return ;
+}
