+
+void  RingBuffer::putData(Buffer *buf,int wake)
+{
+    mDataLock.lock();
+    if(isFull()){
+        // to do error
+    }
+    wr_index += buf->mSize;
+    if(wake){
+        mDataCond.signal();
+        //ALOGD("putData wr signal,wr_index:%zd,rd_index:%zd,this %p",wr_index,rd_index,this);
+    }
+    mDataLock.unlock();
+    mLock_wr.unlock();
+    return ;
+}
