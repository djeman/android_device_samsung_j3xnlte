+double FLVExtractor::amf_get_doublenum(uint32_t offset) {
+    uint8_t buffer[8];
+    mDataSource->readAt(offset, buffer, 1);
+    if(buffer[0]!=0) {
+        ALOGE("amf_get_doublenum not double num");
+        return 0;
+    }
+    mDataSource->readAt(offset+1, buffer, 8);
+    return av_int2dbl(U64_AT(buffer));
+}
